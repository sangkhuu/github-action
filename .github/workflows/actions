name: Actions Workflow

on:
  push:
    branches:
      - master
      - develop
      - 'release/**' # match all release/xxxx/xx
      - '!hotfix/**' # ignore hotfix
    tags:
      - 1.0.0
      - v1.*
    paths:
      - '**.js'
      

jobs:
  run-github-actions:
    runs-on: ubuntu-latest
    steps:
      - name: List Files 1
        run: |
          pwd
          ls -a
          whoami
